{
	"compilerOptions": {
		"declaration": true,
		"module": "ES2022",
		"target": "ES2019",
		"outDir": "./out-tsc",
		"sourceMap": true,
		"strict": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	}
}