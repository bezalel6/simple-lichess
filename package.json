{
  "name": "simple-lichess-api",
  "description": "TypeScript library for Lichess API with streaming support, game analysis, and OAuth authentication",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "types": "out-tsc/index.d.ts",
  "type": "commonjs",
  "watch": {
    "test": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "build": "rimraf dist out-tsc && tsc && rollup -c rollup.config.js",
    "start": "npm run build && node ./dist/bundle.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "/dist",
    "/out-tsc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bezalel6/simple-lichess"
  },
  "keywords": [
    "lichess",
    "chess",
    "api",
    "typescript",
    "streaming",
    "pgn",
    "oauth",
    "games",
    "analysis"
  ],
  "author": "Kenneth Reilly @8_bit_hacker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bezalel6/simple-lichess"
  },
  "homepage": "https://github.com/bezalel6/simple-lichess#readme",
  "dependencies": {
    "@bity/oauth2-auth-code-pkce": "^2.13.0",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rimraf": "^5.0.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "typescript": "^5.0.0"
  }
}
